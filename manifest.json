{
    "name": "You Are Not Allowed",
    "description": "This is a chrome extension to restrict anyone on using any url",
    "manifest_version": 3,
    "author": "Deepsk Bhatt",
   "version": "0.0.1.0",
   "content_scripts": [{
        "matches" : ["<all_urls>"],
        "js" : ["content.js"]
   }
   ],

   "action": {
     "default_popup": "Index.html",
     "default_title": "Not Allowed Activated"
   }
}